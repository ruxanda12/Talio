<AnchorPane fx:id="list" prefHeight="509.0" prefWidth="332.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.Lists.ListCtrl">
   <children>
      <BorderPane layoutX="17.0" layoutY="50.0" prefHeight="446.0" prefWidth="300.0">
         <top>
            <Label fx:id="title" alignment="TOP_CENTER" contentDisplay="CENTER" prefHeight="13.0" prefWidth="294.0" text="List" textAlignment="CENTER" BorderPane.alignment="CENTER">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
         </top>
         <center>
            <VBox fx:id="taskList" alignment="CENTER_RIGHT" prefHeight="368.0" prefWidth="300.0" BorderPane.alignment="CENTER" />
         </center>
         <bottom>
            <Button fx:id="addTask" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#addTask" text="Add Task" BorderPane.alignment="CENTER" />
         </bottom>
      </BorderPane>
      <Button layoutX="213.0" layoutY="14.0" mnemonicParsing="false" onAction="#deleteFelloList" prefWidth="80.0" text="Delete List" />
      <Button layoutX="39.0" layoutY="14.0" mnemonicParsing="false" onAction="#editFelloList" prefWidth="80.0" text="Edit List" />
